{
  "name": "zenpay-api-docs",
  "version": "1.0.0",
  "description": "TravelPay API documentation tools and specifications",
  "scripts": {
    "getspec": "bun run swagger-downloader.ts",
    "validate": "bun run validate-spec.ts",
    "analyze": "bun run analyze-spec.ts",
    "enhance": "bun run enhance-spec.ts",
    "enhance:preview": "bun run enhance-spec.ts --preview",
    "enhance:all": "bun run enhance-spec.ts --all --apply",
    "sdk": "bun run generate-sdk.ts",
    "docs": "bun run serve-docs.ts",
    "diff": "bun run show-diff.ts",
    "help": "bun run show-help.ts",
    "compare": "bun run compare-specs.ts",
    "deep-compare": "bun run deep-compare-specs.ts",
    "comprehensive-diff": "bun run comprehensive-diff.ts",
    "verify": "bun run verify-comparison.ts",
    "normalize": "bun run normalize-and-diff.ts",
    "postman:download": "bun run postman-download.ts",
    "postman:convert": "bun run postman-to-openapi31.ts",
    "postman:upload": "bun run postman-upload.ts",
    "postman:workspaces": "bun run postman-upload.ts --list-workspaces",
    "postman:collections": "bun run postman-upload.ts --list-collections",
    "postman:list-sections": "bun run postman-extract-section.ts --list",
    "postman:extract": "bun run postman-extract-section.ts",
    "postman:full": "bun run postman-download.ts && bun run postman-to-openapi31.ts",
    "extract:merchant": "bun run extract-merchant-apis.ts",
    "compare:full": "bun run compare-postman-travelpay.ts",
    "compare:full:cached": "bun run compare-postman-travelpay.ts --use-cache",
    "compare:full:merchant": "bun run compare-postman-travelpay.ts --merchant-only",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@scalar/api-reference": "^1.39.3",
    "@scalar/openapi-parser": "^0.23.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "postman-to-openapi": "^1.7.3"
  },
  "keywords": [
    "openapi",
    "swagger",
    "api",
    "documentation",
    "travelpay"
  ],
  "author": "TravelPay",
  "license": "MIT"
}
