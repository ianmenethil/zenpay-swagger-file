{
  "name": "zenpay-api-docs",
  "version": "1.0.0",
  "description": "TravelPay API documentation tools and specifications",
  "scripts": {
    "getspec": "bun run swagger-downloader.ts",
    "analyze": "bun run analyze-spec.ts",
    "enhance": "bun run enhance-spec.ts",
    "enhance:preview": "bun run enhance-spec.ts --preview",
    "enhance:all": "bun run enhance-spec.ts --all --apply",
    "diff": "bun run show-diff.ts",
    "verify": "node verify-conversion.js",
    "split": "bun run split-spec.ts",
    "bundle": "bun run bundle-spec.ts",
    "deref": "bun run deref-spec.ts",
    "validate": "bun run validate-spec.ts",
    "lint": "bun run lint-spec.ts",
    "format": "bun run format-spec.ts",
    "help": "bun run show-help.ts"
  },
  "keywords": [
    "openapi",
    "swagger",
    "api",
    "documentation",
    "travelpay"
  ],
  "author": "TravelPay",
  "license": "MIT"
}
