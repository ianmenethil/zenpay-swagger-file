# TravelPay API Configuration
# Get your credentials from: https://portal.travelpay.com/settings/api-keys

# TravelPay Sandbox
TRAVELPAY_SANDBOX_API_KEY=your_sandbox_api_key
TRAVELPAY_SANDBOX_USERNAME=your_sandbox_username
TRAVELPAY_SANDBOX_PASSWORD=your_sandbox_password

# TravelPay Production
TRAVELPAY_PROD_API_KEY=your_prod_api_key
TRAVELPAY_PROD_USERNAME=your_prod_username
TRAVELPAY_PROD_PASSWORD=your_prod_password

# TravelPay UAT
TRAVELPAY_UAT_API_KEY=your_uat_api_key
TRAVELPAY_UAT_USERNAME=your_uat_username
TRAVELPAY_UAT_PASSWORD=your_uat_password

# Postman API Configuration
# Get your API key from: https://web.postman.co/settings/me/api-keys
POSTMAN_API_KEY=your_postman_api_key

# Postman public collection access key (for downloading shared collections)
POSTMAN_ACCESS_KEY=your_collection_access_key
